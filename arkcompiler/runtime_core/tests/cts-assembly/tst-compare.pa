#
# Tests on Compare with CC_TST_EQ/CC_TST_NE condition code.
#
.function i32 testVarsEqZeroI32(i32 a0, i32 a1) {
    lda a0
    and2 a1
    jeqz ret_true
    ldai 0
    return
ret_true:
    ldai 1
    return
}

.function i32 testVarsNeZeroI32(i32 a0, i32 a1) {
    lda a0
    and2 a1
    jnez ret_true
    ldai 0
    return
ret_true:
    ldai 1
    return
}

.function i32 testVarSmallImmEqZeroI32(i32 a0) {
