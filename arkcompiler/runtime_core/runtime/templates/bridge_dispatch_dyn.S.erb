% calls = get_call_insns()
% classified_calls = classify_calls(calls)
% formats = classified_calls.keys
%
% formats.each do |fmt|
%   insns = classified_calls[fmt]
%   insns.each do |insn|
    <%= cmp_opcode(insn.opcode_idx) %>
    <%= jump_eq(".Lhandle_#{insn.opcode_idx}") %>
%   end
% end

% formats.each do |fmt|
%   insns = classified_calls[fmt]
%   insns.each do |insn|
      .Lhandle_<%= insn.opcode_idx %>:
      <%= save_param(insn.operands.length) %>
      <%= jump(".Lhandle_#{fmt}") %>
%   end
% end

% formats.each do |fmt|
.Lhandle_<%= fmt %>:
#include "<%= handler_path(fmt) %>"
% end
