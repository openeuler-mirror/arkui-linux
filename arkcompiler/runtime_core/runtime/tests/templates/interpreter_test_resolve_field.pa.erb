<%# METHOD_COUNT = panda_file::MAX_INDEX_16 %>
% METHOD_COUNT = 65535

.record R1 {
    i32 f <static>
}

.function void R1.cctor() <cctor> {
    ldai 10
    ststatic R1.f
    return.void
}

.function i32 R1.get() {
    ldstatic R1.f
    return
}

% for i in 1..METHOD_COUNT do
.function void R1.f<%=i%>() {
    call R1.f<%=i%>
    return.void
}
% end

.record R2 {
    i32 f <static>
}

.function void R2.cctor() <cctor> {
    ldai 20
    ststatic R2.f
    return.void
}

.function i32 R2.get() {
    ldstatic R2.f
    return
}

% for i in 1..METHOD_COUNT do
.function void R2.f<%=i%>() {
    call R2.f<%=i%>
    return.void
}
% end